                    app:layout_constraintTop_toTopOf="@+id/calculations44_input4" />

                <EditText
                    android:id="@+id/calculations44_input6"
                    android:layout_width="120dp"
                    android:layout_height="30dp"
                    android:layout_marginTop="16dp"
                    android:background="@drawable/background_black"
                    android:ems="10"
                    android:inputType="number"
                    android:textAlignment="center"
                    android:textColor="@color/white"
                    app:layout_constraintBottom_toBottomOf="@+id/txt6_calculations44"
                    app:layout_constraintEnd_toEndOf="@+id/calculations44_input1"
                    app:layout_constraintTop_toBottomOf="@+id/calculations44_input5" />

                <EditText
                    android:id="@+id/calculations44_input4"
                    android:layout_width="120dp"
                    android:layout_height="30dp"
                    android:layout_marginTop="16dp"
