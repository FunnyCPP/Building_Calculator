                <TextView
                    android:id="@+id/txt4_calculations44"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:gravity="center|start"
                    android:minHeight="30dp"
                    android:text="@string/calculation44_text4"
                    app:layout_constraintEnd_toEndOf="@+id/txt1_calculations44"
                    app:layout_constraintStart_toStartOf="@+id/txt1_calculations44"
                    app:layout_constraintTop_toBottomOf="@+id/txt3_calculations44" />

                <TextView
                    android:id="@+id/txt5_calculations44"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:gravity="center|start"
                    android:minHeight="30dp"
                    android:text="@string/calculation44_text5"
                    app:layout_constraintEnd_toEndOf="@+id/txt1_calculations44"
